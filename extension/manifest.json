{
  "manifest_version": 2,
  "name": "WatchParty Controls",
  "version": "1.0",
  "description": "Adds a floating back/home bar that hides in fullscreen",
  "permissions": ["storage", "tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["controls.js"],
      "css": ["controls.css"],
      "run_at": "document_end"
    }
  ]
}
